extends ../layout
block head
    title  入驻申请
    link(href='/web/css/apply.css', rel='stylesheet')
block body
    .html-title.service
    .container
        .bread-crumb
            span.icon
            a(href='/') 首页
            span &gt;
            a(href='/web/serviceCenter.html') 服务中心
            span &gt;
            a(href='javascript:void(0)') 入驻申请
    .container.bg_white.huntApply
        .col-xs-12
            p.msg#notice
        .col-xs-12
            form.form-horizontal#form(name="form",role="form",method="POST")
                .col-xs-12
                    label.read
                        input#mustRead(type='checkbox',name='mustRead')
                        span 已阅读
                .form-group
                    .col-xs-11
                        .col-xs-3.text-right
                            label.control-label
                                span.star
                                span 申请人：
                        .col-xs-8
                            input.form-control(type='text',name='chineseName')
                .form-group
                    .col-xs-11
                        .col-xs-3.text-right
                            label.control-label
                                span.star
                                span E-mail：
                        .col-xs-8
                            input.form-control(type='text',name='email', placeholder='请输入邮箱')
                .form-group
                    .col-xs-11
                        .col-xs-3.text-right
                            label.control-label
                                span.star
                                span 公司名称：
                        .col-xs-8
                            input.form-control(type='text',name='company',placeholder='请输入公司名称')
                .form-group
                    .col-xs-11
                        .col-xs-3.text-right
                            label.control-label
                                span.star
                                span 联系方式：
                        .col-xs-8
                            input.form-control(type='text',name='contact')
                .form-group
                    .col-xs-11
                        .col-xs-3.text-right
                            label.control-label
                                span.star
                                span 企业规模：
                        .col-xs-8
                            select.form-control#staff(name='sStafffId')
                .form-group
                    .col-xs-11
                        .col-xs-3.text-right
                            label.control-label
                                span.star
                                span 产业类别：
                        .col-xs-8
                            select.form-control#type(name='sTypeId')
                .form-group
                    .col-xs-11
                        .col-xs-3.text-right
                            label.control-label
                                span.star
                                span 园区选择：
                        .col-xs-8#parkCont
                .form-group
                    .col-xs-11
                        .col-xs-3.text-right
                            label.control-label
                                span.star
                                span 需求面积：
                            p.text-right.mr16 (平方米)
                        .col-xs-8
                            input.form-control(type='text',name='area')
                .form-group
                    .col-xs-11
                        .col-xs-3.text-right
                            label.control-label 需求说明：
                        .col-xs-8
                            textarea.form-control(type='text',name='details',rows='5')
                .form-group
                    .col-xs-11
                        .col-xs-3.text-right
                            label.control-label 备注：
                        .col-xs-8
                            textarea.form-control(type='text',name='memo',rows='5')
                div.display_n#comment
                    .col-xs-11
                        .col-xs-3.text-right
                            label.control-label 评价信息：
                        .col-xs-9
                            .col-xs-5
                                p#comment1
                            .col-xs-2.ratingScore.ratingScore-duration
                                span#duration
                                p.ratingText 响应速度
                            .col-xs-2.ratingScore.ratingScore-attitude
                                span#attitude
                                p.ratingText 服务态度
                            .col-xs-2.ratingScore.ratingScore-quality
                                span#quality
                                p.ratingText 服务质量
                .form-group
                    .col-xs-12.text-center
                        button.btn.btn-blue.submit(type="submit") 提交


    include ../include/footer
    script(src="/web/js/hatchApply.js")

